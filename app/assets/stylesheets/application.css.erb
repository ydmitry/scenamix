}

.btn-custom {
  -webkit-box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.4);
  -moz-box-shadow:    0px 0px 3px rgba(0, 0, 0, 0.4);
  box-shadow:         0px 0px 3px rgba(0, 0, 0, 0.4);
  color: #FFF;
  text-shadow: 0px -1px 0px #333;

  background-color: #029dd4;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#21c1f3), to(#0386bc)); /* Chrome, Safari 4+ */
  background-image: -webkit-linear-gradient(top, #21c1f3 0%, #22acdd 50%, #029dd4 50%, #0386bc 100%); /* Chrome 10-25, iOS 5+, Safari 5.1+ */
  background-image:    -moz-linear-gradient(top, #21c1f3 0%, #22acdd 50%, #029dd4 50%, #0386bc 100%); /* Firefox 3.6-15 */
  background-image:      -o-linear-gradient(top, #21c1f3 0%, #22acdd 50%, #029dd4 50%, #0386bc 100%); /* Opera 11.10-12.00 */
  background-image:         linear-gradient(to bottom, #21c1f3 0%, #22acdd 50%, #029dd4 50%, #0386bc 100%); /* Chrome 26, Firefox 16+, IE 10+, Opera 12.10+ */

  display: inline-block;
  border-radius: 5px;
  padding: 5px 16px;
  border: none;
  vertical-align: middle;
}

.btn-custom:hover, .btn-custom:active {
  color: #FFF;
  background-color: #028dc4;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#11b1e3), to(#0376ac)); /* Chrome, Safari 4+ */
  background-image: -webkit-linear-gradient(top, #11b1e3 0%, #129ccd 50%, #028dc4 50%, #0376ac 100%); /* Chrome 10-25, iOS 5+, Safari 5.1+ */
  background-image:    -moz-linear-gradient(top, #11b1e3 0%, #129ccd 50%, #028dc4 50%, #0376ac 100%); /* Firefox 3.6-15 */
  background-image:      -o-linear-gradient(top, #11b1e3 0%, #129ccd 50%, #028dc4 50%, #0376ac 100%); /* Opera 11.10-12.00 */
  background-image:         linear-gradient(to bottom, #11b1e3 0%, #129ccd 50%, #028dc4 50%, #0376ac 100%); /* Chrome 26, Firefox 16+, IE 10+, Opera 12.10+ */

  text-decoration: none;
}


.btn-custom-plus:before {
  content: "+";
  font-size: 1.5em;
  padding-right: 7px;
}

#scenes-new-head .btn-custom {
  padding: 4px 5px 4px;
  margin: 0 5px;
}

#scenes-new-head {
  line-height: 19px;
}


